function assert(x: boolean): void {
    if (x) {
        putchar(46);
    } else {
        putchar(70);
    }
}
function factorialLoop(n: number): number {
    var result = 1;
    while (n != 1) {
        result = result * n;
        n = n - 1;
    }
    return result;
}
function main() {
    assert(720 == factorialLoop(6));
}
